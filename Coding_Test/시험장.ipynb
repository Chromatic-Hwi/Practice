   "source": [
    "# 2. 숫자 카드 게임 (그리디)\n",
    "\n",
    "M,N = map(int, input('행과 열의 숫자를 스페이스로 구분하여 입력 : ').split())\n",
    "\n",
    "list2=[]\n",
    "for n in range(N):\n",
    "    list1 = list(map(int, input(str(M)+'개의 숫자를 스페이스로 구분하여 입력').split()))\n",
    "    min1 = min(list1)\n",
    "    list2.append(min1)\n",
    "final_min=min(list2)\n",
    "\n",
    "print('\\nAnswer >>', final_min)"
   ]
  }
